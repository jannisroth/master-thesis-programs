<Application x:Class="EcoConf.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             xmlns:local="clr-namespace:EcoConf"
             Startup="StartApplication">
    <Application.Resources>
        <sys:Int32 x:Key="inputDllSize">60</sys:Int32>
        <sys:Int32 x:Key="inputConfigurationSize">30</sys:Int32>
        <sys:Int32 x:Key="outputDllSize">200</sys:Int32>
        <sys:Int32 x:Key="outputConfigurationSize">100</sys:Int32>
        <sys:String x:Key="nameIntake">Zuluft</sys:String>
        <sys:String x:Key="nameOuttake">Abluft</sys:String>
    </Application.Resources>
</Application>

